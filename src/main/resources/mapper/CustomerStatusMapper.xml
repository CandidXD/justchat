<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.candidxd.justchat.dao.CustomerStatusDao">    <resultMap id="BaseResultMap" type="com.candidxd.justchat.bean.CustomerStatus">        <id column="STATUS_ID" jdbcType="INTEGER" property="id"/>        <result column="CUS_UID" jdbcType="VARCHAR" property="customerUid"/>        <result column="STATUS_STATE" jdbcType="VARCHAR" property="state"/>        <result column="STATUS_STATE_ID" jdbcType="INTEGER" property="stateId"/>        <result column="STATUS_LOGIN_TIME" jdbcType="VARCHAR" property="loginTime"/>        <result column="CUS_GENDER" jdbcType="INTEGER" property="customerGender"/>    </resultMap>    <insert id="add" parameterType="com.candidxd.justchat.bean.CustomerStatus">        INSERT INTO customer_status (CUS_UID, STATUS_STATE, STATUS_STATE_ID, STATUS_LOGIN_TIME, CUS_GENDER)        VALUES (#{customerUid}, #{state}, #{stateId}, #{loginTime}, #{customerGender})    </insert>    <select id="list" resultMap="BaseResultMap">        SELECT *        FROM customer_status    </select>    <update id="update" parameterType="com.candidxd.justchat.bean.CustomerStatus">        UPDATE customer_status        SET CUS_UID           = #{customerUid},            STATUS_STATE      = #{state},            STATUS_STATE_ID   = #{stateId},            STATUS_LOGIN_TIME = #{loginTime},            CUS_GENDER        = #{customerGender}        WHERE CUS_UID = #{customerUid}    </update>    <select id="matching" parameterType="com.candidxd.justchat.bean.CustomerStatus" resultMap="BaseResultMap">        SELECT *        FROM customer_status        WHERE STATUS_STATE_ID = 2          AND CUS_UID != #{customerUid}    </select>    <select id="getOne" parameterType="com.candidxd.justchat.bean.CustomerStatus" resultMap="BaseResultMap">        SELECT *        FROM customer_status        WHERE CUS_UID = #{customerUid}    </select></mapper>