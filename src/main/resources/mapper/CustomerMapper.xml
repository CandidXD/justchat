<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.candidxd.justchat.dao.CustomerDao">    <resultMap id="BaseResultMap" type="com.candidxd.justchat.bean.Customer">        <id column="CUS_ID" jdbcType="INTEGER" property="id"/>        <result column="CUS_UID" jdbcType="INTEGER" property="uid"/>        <result column="CUS_IP" jdbcType="INTEGER" property="ip"/>        <result column="CUS_PROVINCE" jdbcType="VARCHAR" property="province"/>        <result column="CUS_CITY" jdbcType="VARCHAR" property="city"/>        <result column="CUS_CREATE_TIME" jdbcType="VARCHAR" property="createTime"/>        <result column="CUS_GENDER" jdbcType="INTEGER" property="gender"/>        <result column="CUS_AGE" jdbcType="INTEGER" property="age"/>    </resultMap>    <insert id="add" parameterType="com.candidxd.justchat.bean.Customer">        INSERT INTO customer (CUS_UID, CUS_IP, CUS_PROVINCE, CUS_CITY, CUS_CREATE_TIME, CUS_GENDER, CUS_AGE)        VALUES (#{uid}, #{ip}, #{province}, #{city}, #{createTime}, #{gender}, #{age})    </insert>    <select id="list" resultMap="BaseResultMap">        SELECT *        FROM customer    </select>    <update id="update" parameterType="com.candidxd.justchat.bean.Customer">        UPDATE customer        SET CUS_UID         = #{uid},            CUS_IP          = #{ip},            CUS_PROVINCE    = #{province},            CUS_CITY        = #{city},            CUS_CREATE_TIME = #{createTime},            CUS_GENDER      = #{gender},            CUS_AGE         = #{age}        WHERE CUS_UID = #{uid}    </update>    <select id="getOne" parameterType="com.candidxd.justchat.bean.Customer" resultMap="BaseResultMap">        SELECT *        FROM customer        WHERE CUS_UID = #{uid}    </select></mapper>